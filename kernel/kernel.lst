828F: C658    			add 	$58
8291: 67      			ld 		h,a
              	
8292: 7A      			ld 		a,d 								; get current colour
8293: E607    			and 	7  									; mask 0..2
8295: F640    			or 		$40  								; make bright
8297: 77      			ld 		(hl),a 								; store it.	
