              	;		Author :	Paul Robson (paul@robsons.org.uk)
              	;		Date : 		22nd November 2018
              	;		Purpose :	Dictionary handler.
              	;
              	; ***************************************************************************************
              	; ***************************************************************************************
              	
              	; ***********************************************************************************************
              	;
              	;		Add Dictionary Word. Name is string at BC ends in $80-$FF, uses the current page/pointer
              	;		values. A identifies whether MACRO ($80) or FORTH $(00)
              	;
              	; ***********************************************************************************************
              	
841E:         	DICTAddWord:
841E: F5      			push 	af 									; registers to stack.
841F: C5      			push 	bc
8420: D5      			push 	de
8421: E5      			push	hl
8422: DDE5    			push 	ix
              	
8424: C5      			push 	bc 									; save word address.
8425: 4F      			ld 		c,a 								; put the dictionary marker in C
8426: 06FF    			ld 		b,-1								; put length of string in B
8428:         	__DICTAddGetLength:
8428: 23      			inc 	hl
8429: 04      			inc 	b
842A: CB7E    			bit 	7,(hl)
842C: 28FA    			jr 		z,__DICTAddGetLength
842E: E1      			pop 	hl 									; restore word address.
              	
842F: 3E20    			ld 		a,DictionaryPage					; switch to dictionary page
8431: CD3480  			call 	PAGESwitch
              	
8434: DD2100C0			ld 		ix,$C000							; IX = Start of dictionary
              	
8438:         	__DICTFindEndDictionary:
8438: DD7E00  			ld 		a,(ix+0) 							; follow down chain to the end
843B: B7      			or 		a
843C: 2807    			jr 		z,__DICTCreateEntry
843E: 5F      			ld 		e,a
843F: 1600    			ld 		d,0
8441: DD19    			add 	ix,de
8443: 18F3    			jr 		__DICTFindEndDictionary
              	
8445:         	__DICTCreateEntry:
8445: 78      			ld 		a,b
8446: C605    			add 	a,5
8448: DD7700  			ld 		(ix+0),a 							; offset is length + 5
              	
844B: 3A078B  			ld 		a,(SINextFreeCodePage)				; code page
844E: DD7701  			ld 		(ix+1),a
8451: ED5B058B			ld 		de,(SINextFreeCode)					; code address
8455: DD7302  			ld 		(ix+2),e
8458: DD7203  			ld 		(ix+3),d 
              	
845B: 79      			ld 		a,c 								; get FORTH/MACRO flag
845C: E680    			and 	$80
845E: B0      			or 		b 									; or length in
845F: DD7704  			ld 		(ix+4),a 							; length (0..5) forth/macro (7)
              	
8462: EB      			ex 		de,hl 								; put name in DE
8463: 13      			inc 	de 									; skip over tag.
8464:         	__DICTAddCopy:
8464: 1A      			ld 		a,(de) 								; copy byte over as 7 bit ASCII.
8465: DD7705  			ld 		(ix+5),a
8468: DD23    			inc 	ix 									
846A: 13      			inc 	de
846B: 10F7    			djnz	__DICTAddCopy 						; until string is copied over.
              	
846D: DD360500			ld 		(ix+5),0 							; write end of dictionary zero.
              	
8471: CD4D80  			call 	PAGERestore
8474: DDE1    			pop 	ix 									; restore and exit
8476: E1      			pop 	hl
8477: D1      	 		pop 	de
8478: C1      			pop 	bc
8479: F1      			pop 	af
847A: C9      			ret
              	
              	; ***********************************************************************************************
              	;
              	;			Find word in dictionary. BC points to name. A is the mask for flag/macro.
              	;			On exit, HL is the address and E the page number with CC if found, 
              	;			CS set and HL=DE=0 if not found.
              	;
              	; ***********************************************************************************************
              	
847B:         	DICTFindWord:
847B: C5      			push 	bc 								; save registers - return in EHL Carry
847C: DDE5    			push 	ix
              	
847E: 60      			ld 		h,b 							; put address of name in HL. 
847F: 69      			ld 		l,c
              	
8480: 4F      			ld 		c,a 							; macro forth flag in C
              	
8481: 3E20    			ld 		a,DictionaryPage 				; switch to dictionary page
8483: CD3480  			call 	PAGESwitch
              	
8486: DD2100C0			ld 		ix,$C000 						; dictionary start			
848A:         	__DICTFindMainLoop:
848A: DD7E00  			ld 		a,(ix+0)						; examine offset, exit if zero.
848D: B7      			or 		a
848E: 283E    			jr 		z,__DICTFindFail
              	
8490: DD7E04  			ld 		a,(ix+4) 						; get the flag/macro (and length) byte
8493: A9      			xor 	c 								; xor with the mask
8494: E680    			and 	$80 							; only interested in bit 7
8496: 202D    			jr 		nz,__DICTFindNext 				; so if different, go to next.
              	
8498: DDE5    			push 	ix 								; save pointers on stack.
849A: E5      			push 	hl 
              	
849B: DD7E04  			ld 		a,(ix+4) 						; characters to compare
849E: E63F    			and 	$3F
84A0: 47      			ld 		b,a
84A1: 23      			inc 	hl 								; skip over tag
84A2:         	__DICTCheckName:
84A2: DD7E05  			ld 		a,(ix+5) 						; compare dictionary vs character.
84A5: BE      			cp 		(hl) 							; compare vs the matching character.
84A6: 201A    			jr 		nz,__DICTFindNoMatch 			; no, not the same word.
84A8: 23      			inc 	hl 								; HL point to next character
84A9: DD23    			inc 	ix
84AB: 10F5    			djnz 	__DICTCheckName
              	
84AD: CB7E    			bit 	7,(hl)							; if so, see if the next one is EOW
84AF: 2811    			jr 		z,__DICTFindNoMatch 			; if not , bad match.
              	
84B1: E1      			pop 	hl 								; Found a match. restore HL and IX
84B2: DDE1    			pop 	ix
84B4: 1600    			ld 		d,0 							; D = 0 for neatness.
84B6: DD5E01  			ld 		e,(ix+1)						; E = page
84B9: DD6E02  			ld 		l,(ix+2)						; HL = address
84BC: DD6603  			ld 		h,(ix+3)		
84BF: AF      			xor 	a 								; clear the carry flag.
84C0: 1813    			jr 		__DICTFindExit
              	
84C2:         	__DICTFindNoMatch:								; this one doesn't match.
84C2: E1      			pop 	hl 								; restore HL and IX
84C3: DDE1    			pop 	ix
84C5:         	__DICTFindNext:
84C5: DD5E00  			ld 		e,(ix+0)						; DE = offset
84C8: 1600    			ld 		d,$00
84CA: DD19    			add 	ix,de 							; next word.
84CC: 18BC    			jr 		__DICTFindMainLoop				; and try the next one.
              	
84CE:         	__DICTFindFail:
84CE: 110000  			ld 		de,$0000 						; return all zeros.
84D1: 210000  			ld 		hl,$0000
84D4: 37      			scf 									; set carry flag
84D5:         	__DICTFindExit:
84D5: F5      			push 	af
84D6: CD4D80  			call 	PAGERestore
84D9: F1      			pop 	af
84DA: DDE1    			pop 	ix 								; pop registers and return.
84DC: C1      			pop 	bc
84DD: C9      			ret
              			include "compiler/loader.asm"
              	; ********************************************************************************************************
              	; ********************************************************************************************************
              	;
              	;		Name : 		loader.asm
              	;		Author : 	Paul Robson (paul@robsons.org.uk)
              	;		Purpose : 	Source loader
              	;		Date : 		19th November 2018
              	;
              	; ********************************************************************************************************
              	; ********************************************************************************************************
              	
              	; ********************************************************************************************************
              	;
              	;									Load the bootstrap page
              	;
              	; ********************************************************************************************************
              	
84DE:         	LOADBootstrap:
84DE: 210000  			ld 		hl,$0000 							; get SP, the operating stack, into DE
84E1: 39      			add 	hl,sp
84E2: EB      			ex 		de,hl 
              	
84E3: 31807D  			ld 		sp,LOADStack 						; the stack used while loading.
              	
84E6: 3E22    			ld 		a,BootstrapPage 					; set the current page to bootstrap page.
84E8: CD3480  			call 	PAGESwitch
84EB: DD2100C0			ld 		ix,$C000 							; current section being loaded.
              	;
              	;		Once here for every 'chunk'. We copy the text to the editor buffer in 
              	;		chunks (currently 1024 bytes) until we've done all 16k of the page.
              	;
84EF:         	__LOADBootLoop:
              	
84EF: C5      			push 	bc 									; save registers
84F0: D5      			push 	de
84F1: E5      			push 	hl
              	
84F2: DDE5    			push 	ix 									; HL = Current Section in IX.
84F4: E1      			pop 	hl
84F5: 11087B  			ld 		de,EditBuffer  						; Copy to edit buffer 1/2k (512 bytes) of code.
84F8: 010002  			ld 		bc,512
84FB: EDB0    			ldir 	
              	
84FD: 2600    			ld 		h,0 								; Progress prompt at the screen top.
84FF: DD7C    			ld 		a,ixh
8501: 0F      			rrca
8502: E61F    			and 	31
8504: 6F      			ld 		l,a
8505: 112A05  			ld 		de,$052A
8508: CD2281  			call 	GFXWriteCharacter
              	
850B: E1      			pop 	hl 									; restore registers
850C: D1      			pop 	de
850D: C1      			pop 	bc
              	
850E: 01087B  			ld 		bc,EditBuffer 						; now scan the edit buffer
8511: CD2185  			call 	LOADScanBuffer 
              	
8514: 010002  			ld 		bc,512 								; add 512 size to IX
8517: DD09    			add 	ix,bc
8519: 30D4    			jr 		nc,__LOADBootLoop 					; will cause carry to occur on overflow.
              	
851B:         	__LOADEnds:
851B: CD4D80  			call 	PAGERestore 						; restore page
851E: C32080  			jp 		HaltZ80 							; and stop
              			
              	; ********************************************************************************************************
              	;
              	;		  Process (compiling) the text at BC. The current stack (uncached) is in DE
              	; 
              	; ********************************************************************************************************
              	
8521:         	LOADScanBuffer:
8521: F5      			push 	af
8522: C5      			push 	bc
8523: DDE5    			push 	ix
              	
8525:         	__LOADScanLoop:
8525: 0A      			ld 		a,(bc) 								; look at tage
8526: FEFF    			cp 		$FF 								; was it $FF - end of page marker ?
8528: 280B    			jr 		z,__LOADScanExit 					; if so, we are done.
              	
852A: CD3A85  			call 	COMCompileExecute 					; execute text at BC, 
              														; uncached stack in DE.
              	
852D:         	__LOADNextWord: 									; look for the next bit 7 high.
852D: 03      			inc 	bc 									; advance forward to next word.
852E: 0A      			ld 		a,(bc)
852F: CB7F    			bit 	7,a
8531: 28FA    			jr 		z,__LOADNextWord
8533: 18F0    			jr 		__LOADScanLoop 
              	
8535:         	__LOADScanExit:
8535: DDE1    			pop 	ix
8537: C1      			pop 	bc
8538: F1      			pop 	af
8539: C9      			ret
              			include "compiler/compiler.asm"
              	; ********************************************************************************************************
              	; ********************************************************************************************************
              	;
              	;		Name : 		compiler.asm
              	;		Author : 	Paul Robson (paul@robsons.org.uk)
              	;		Purpose : 	Source loader
              	;		Date : 		23rd November 2018
              	;
              	; ********************************************************************************************************
              	; ********************************************************************************************************
              	
              	; ********************************************************************************************************
              	; ********************************************************************************************************
              	;
              	;	 Handle the word in BC. The current uncached stack is at DE (e.g. TOS is on the stack)
              	;					Returns DE possibly changed depending on what's been done.
              	;
              	;	Note each level of stack needs its own stack. So we really only want one level of
              	; 	reentrancy. The stack is only needed for messing around in here though.
              	;
              	; ********************************************************************************************************
              	; ********************************************************************************************************
              	
853A:         	COMCompileExecute:
              	
853A: C5      			push 	bc
853B: E5      			push 	hl 									; save BC, HL and IX.
853C: DDE5    			push 	ix
              	
853E: D5      			push 	de 									; put the "data stack" in ix
853F: DDE1    			pop 	ix
              	
8541: 0A      			ld 		a,(bc) 								; read the tag.
8542: FE84    			cp 		$84
8544: CA5A85  			jp 		z,__COMCE_Compiler_Green
8547: FE86    			cp 		$86
8549: CA7385  			jp 		z,__COMCE_Executor_Yellow
              	;
              	;						  Come here to exit successfully
              	;
854C:         	__COMExit: 
854C: DDE5    			push 	ix 									; transfer the "data stack" to DE
854E: D1      			pop 	de
              	
854F: DDE1    			pop 	ix									; restore IX and HL and BC
8551: E1      			pop 	hl
8552: C1      			pop 	bc
8553: C9      			ret
              	;
              	;									Come here to fail
              	;
8554:         	__COMError:
8554: C5      			push 	bc 									; put the word in HL
8555: E1      			pop 	hl
8556: 23      			inc 	hl 									; skip the tag
8557: C31E80  			jp 		ErrorHandler
              	
              	; ********************************************************************************************************
              	;
              	;										Green Compiler
              	;
              	; ********************************************************************************************************
              	
855A:         	__COMCE_Compiler_Green:
855A: 3E80    			ld 		a,$80 								; look for the word in MACRO
855C: CD7B84  			call 	DICTFindWord
855F: D29F85  			jp 		nc,__COMCE_ExecuteEHL 				; if found then *execute* it.
              	
8562: 3E00    			ld 		a,$00 								; look for the word in FORTH
8564: CD7B84  			call 	DICTFindWord
8567: D29485  			jp 		nc,__COMCE_CompileCallEHL 			; if found then compile a call to it.
              	
856A: CDD683  			call 	CONSTConvert 						; try a number.
856D: D28485  			jp 		nc,__COMCE_CompileConstantCode		; if found then compile a push integer
              	
8570: C35485  			jp 		__COMError
              	
              	; ********************************************************************************************************
              	;
              	;										Yellow Compiler
              	;
              	; ********************************************************************************************************
              	
8573:         	__COMCE_Executor_Yellow:
8573: 3E00    			ld 		a,$00 								; look for the word in FORTH
8575: CD7B84  			call 	DICTFindWord
8578: D29F85  			jp 		nc,__COMCE_ExecuteEHL 				; if found then *execute* it.
              	
857B: CDD683  			call 	CONSTConvert 						; try a number.
857E: D2DA85  			jp 		nc,__COMCE_ExecuteConstantCode		; if found then compile a push integer
              	
8581: C35485  			jp 		__COMError
              	
              	; ********************************************************************************************************
              	;
              	;							  Compile code to Push constant HL
              	;
              	; ********************************************************************************************************
              	
8584:         	__COMCE_CompileConstantCode:
8584: 3ED5    			ld 		a,$D5 								; PUSH DE
8586: CD6380  			call 	FARCompileByte
8589: 3E11    			ld 		a,$11 								; LD DE,xxxx
858B: CD6380  			call 	FARCompileByte		
858E: CD7B80  			call 	FARCompileWord 						; the constant
8591: C34C85  			jp 		__COMExit 							; and we are done.
              	
              	; ********************************************************************************************************
              	;
              	;							  Compile code to call code at EHL
              	;
              	; ********************************************************************************************************
              	
8594:         	__COMCE_CompileCallEHL:
              			;
              			;		TODO *** If HL >= $C000 and E != Here.Page then paging call required.
              			;
8594: 3ECD    			ld 		a,$CD 								; call
8596: CD6380  			call 	FARCompileByte		
8599: CD7B80  			call 	FARCompileWord 						; the constant
859C: C34C85  			jp 		__COMExit 							; and we are done.
              	
              	; ********************************************************************************************************
              	;
              	;				    Execute routine at EHL. This needs to be re-entrant
              	;
              	; ********************************************************************************************************
              	
859F:         	__COMCE_ExecuteEHL:
859F: 7B      			ld 		a,e 								; first, switch to that page
85A0: CD3480  			call 	PAGESwitch
85A3: EB      			ex 		de,hl 								; put the call address in DE for a minute
              	
85A4: 210000  			ld 		hl,$0000 							; get the actual SP value.
85A7: 39      			add 	hl,sp
              	
85A8: ED4B2B8B			ld 		bc,(COMXStackPointer)				; Push it on the stack used by Compiler.
85AC: 7D      			ld 		a,l
85AD: 02      			ld 		(bc),a
85AE: 03      			inc 	bc
85AF: 7C      			ld 		a,h
85B0: 02      			ld 		(bc),a
85B1: 03      			inc 	bc
85B2: ED432B8B			ld 		(COMXStackPointer),bc 
              	
85B6: DDE5    			push 	ix 									; put the runtime stack pointer in SP
85B8: E1      			pop 	hl
85B9: F9      			ld 		sp,hl 
              	
85BA: EB      			ex 		de,hl 								; put address back in HL
              	
85BB: D1      			pop 	de  								; cache the TOS in DE.
              	
85BC: CDD985  			call 	__COMCE_CallHL 						; same as CALL (HL)
              	
85BF: D5      			push 	de 									; make the stack uncached.
              	
85C0: 210000  			ld 		hl,$0000 							; copy SP back into IX
85C3: 39      			add 	hl,sp 								; the save place of the working stack.
85C4: E5      			push 	hl
85C5: DDE1    			pop 	ix
              	
85C7: 2A2B8B  			ld 		hl,(COMXStackPointer) 				; retrieve the old stack pointer
85CA: 2B      			dec 	hl
85CB: 56      			ld 		d,(hl)
85CC: 2B      			dec 	hl
85CD: 5E      			ld 		e,(hl)
85CE: 222B8B  			ld 		(COMXStackPointer),hl 
              	
85D1: EB      			ex 		de,hl 								; and put it in SP
85D2: F9      			ld 		sp,hl
              	
85D3: CD4D80  			call 	PAGERestore 						; restore the original page
              	
85D6: C34C85  			jp 		__COMExit							; and we're done :)
              	
85D9:         	__COMCE_CallHL:
85D9: E9      			jp 		(hl)
              	
              	; ********************************************************************************************************
              	;
              	;					Modify the Stack at IX so it has HL on the top now
              	;	
              	; ********************************************************************************************************
              	
85DA:         	__COMCE_ExecuteConstantCode:
85DA: DD75FE  			ld 		(ix-2),l 							; IX is our uncached stack.
85DD: DD74FF  			ld 		(ix-1),h
85E0: DD2B    			dec 	ix
85E2: DD2B    			dec 	ix
85E4: C34C85  			jp 		__COMExit
              	
